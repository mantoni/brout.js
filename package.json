{
  "name": "brout",
  "version": "1.1.0",
  "description": "stdout and stderr for browsers",
  "keywords": [
    "stdout",
    "stderr",
    "browser",
    "browserify",
    "browserify-transform"
  ],
  "author": "Maximilian Antoni <mail@maxantoni.de> (http://maxantoni.de)",
  "contributors": [
    "Deema Yvanow <df.creative@gmail.com>"
  ],
  "homepage": "https://github.com/mantoni/brout.js",
  "main": "./lib/transform.js",
  "browser": "./lib/brout.js",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "lint": "jslint --edition=latest --color \"**/!(*bundle|*-test).js\"",
    "pretest": "npm run lint",
    "test": "browserify -p mocaccino test/brout-test.js | phantomic --debug",
    "just-test": "browserify -p mocaccino test/brout-test.js | phantomic",
    "test-client": "npm run pretest && node client-build test/brout-test.js > test/brout-bundle.js",
    "test-bundle": "browserify test/brout-test.js > test/brout-bundle.js",
    "phantomBuild": "node phantom-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mantoni/brout.js.git"
  },
  "dependencies": {
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "mocha": "^2.3.4",
    "jslint": "^0.8",
    "browserify": "^13.0.0",
    "mocaccino": "^1.7.0",
    "phantomic": "^1.4.0"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT"
}
